{
  "name": "node-express-starter",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "npm run build && node ./dist/app.js",
    "dev": "nodemon --ext js,html --exec babel-node ./src/app.js",
    "build": "cross-env NODE_ENV=production babel ./src --out-dir ./dist && cp -a ./src/public/. ./dist/public ",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "test": "npm run lint && mocha --compilers js:babel-register",
    "clean": "rimraf ./dist",
    "purge": "rm -rf ./node_modules"
  },
  "author": "Evance Soumaoro",
  "license": "ISC",
  "dependencies": {
    "config": "^1.26.1",
    "express": "^4.15.3",
    "nunjucks": "^3.0.1",
    "body-parser": "^1.17.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "should": "^11.2.1",
    "sqlite3": "^3.1.8",
    "standard": "^10.0.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": []
  }
}
